    </div>

    <div class="card">
      <div id="image-course">
        <%= image_tag "rails1.png", alt: "Ruby", class: "img-circle image" %>
      </div>
      <div class="course-infor">
        <div class="status">
          <i class="fa fa-circle init-color" aria-hidden="true"></i> &nbsp;
          New subjects:
          0/
          3
        </div>
        <div class="status">
          <i class="fa fa-circle progress-color" aria-hidden="true"></i> &nbsp;
          Inprogress subjects:
          1/
          3
        </div>
        <div class="status">
          <i class="fa fa-circle finish-color" aria-hidden="true"></i> &nbsp;
          Finished subjects:
          2/
          3
        </div>
        <div class="status">
          <i class="fa fa-circle waiting-color" aria-hidden="true"></i> &nbsp;
          <span class="translation_missing" title="translation missing: en.subjects.waiting_subjects">Waiting Subjects</span>
          0/
          3
        </div>
      </div>
    </div>

    <div class="sector-content">
      <div class="box box-primary">
        <div class="box-header with-border">
          <h3 class="label box-title">Course Notes</h3>
        </div>
        <div class="box-body">
          <div class="subject-note">
            Course's Time is from 2016-09-27 to 2016-10-27
          </div>
        </div>
      </div>
    </div>

    <div class="box box-primary">
      <div class="box-header with-border">
        <h3 class="label box-title">Documents</h3>
      </div>
      <div class="box-body no-padding">
        <ul class="doc-field">
        </ul>
      </div>
    </div>
  </div>
</div>

